    <style>
      .icons-demo {
        display: flex;
        flex-direction: row;
        flex-wrap: wrap;
        background-color: hsla(25, 75%, 80%, 0.3);
      }

      .icons-demo>li {
        flex: 1 0 auto;
        flex-grow: 1 0 2rem;
        text-align: center;
      }

      .icons-demo>li:hover svg {
